              {
                "group": "Commands",
                "icon": "list",
                "pages": [
                  "cli/commands/init",
                  "cli/commands/serve",
                  "cli/commands/deploy",
                  "cli/commands/run",
                  "cli/commands/upload",
                  "cli/commands/start",
                  "cli/commands/setup",
                  "cli/commands/teardown",
                  "cli/commands/template"
                ]
              },
              "cli/examples"
            ]
          }
        ]
      },
