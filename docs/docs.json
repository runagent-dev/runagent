{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "RunAgent",
  "colors": {
    "primary": "#006AD4",
    "light": "#007FFF",
    "dark": "#006AD4"
  },
  "favicon": "/favicon.ico",

  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"    ]
  },
  "navigation": {    
