        "groups": [
          {
            "group": "Overview",
            "pages": [
              "sdk/overview"
            ]
          },
          {
            "group": "Python SDK",
            "pages": [
              "sdk/python/getting-started",
              "sdk/python/sync-client",
              "sdk/python/async-client",
              "sdk/python/streaming",
              "sdk/python/api-reference"
            ]
          },
          {
            "group": "Rust SDK",
            "pages": [
              "sdk/rust/getting-started",
              "sdk/rust/async-operations", 
